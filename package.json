{
  "name": "@raptorthree/scaffold",
  "version": "0.0.1",
  "description": "Scaffold projects from any git repo or local template",
  "type": "module",
  "bin": {
    "scaffold": "./dist/index.js",
    "@raptorthree/scaffold": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun run tsup src/index.ts --format esm --dts --clean",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "scaffold",
    "starter",
    "template",
    "cli"
  ],
  "author": "ash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raptorthree/scaffold"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "citty": "^0.2.0",
    "giget": "^3.1.1",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
